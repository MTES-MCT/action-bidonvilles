{
  "extends": [
    "plugin:vue/essential",
    "eslint:recommended",
    "@vue/airbnb"
  ],
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1
      }
    ],
    "import/no-extraneous-dependencies": 0,
    "import/no-cycle": 0,
    "max-len": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "App": "readonly"
  },
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
